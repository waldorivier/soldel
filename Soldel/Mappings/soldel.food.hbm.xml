<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Soldel" namespace="mupeModel" xmlns="urn:nhibernate-mapping-2.2">
  <class name="food" table="food">
    <id name="food_id" type="Int64">
      <column name="food_id" not-null="true" />
      <generator class="assigned" />
    </id>
    <property name="name" type="String">
      <column name="name" not-null="true" />
    </property>
    <many-to-one name="caterory" class="caterory">
      <column name="category_id" not-null="true" />
    </many-to-one>
    <bag name="l_meal_content" cascade="none" inverse="true" generic="true">
      <key>
        <column name="food_id" />
      </key>
      <one-to-many class="meal_content" />
    </bag>
  </class>
</hibernate-mapping>