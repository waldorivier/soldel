<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Soldel" namespace="mupeModel" xmlns="urn:nhibernate-mapping-2.2">
  <class name="meal" table="meal">
    <id name="meal_id" type="Int64">
      <column name="meal_id" not-null="true" />
      <generator class="assigned" />
    </id>
    <property name="meal_date" type="DateTime">
      <column name="meal_date" not-null="true" />
    </property>
    <property name="meal_code" type="Int64">
      <column name="meal_code" not-null="false" />
    </property>
    <property name="meal_code_str" type="String">
      <column name="meal_code_str" not-null="true" />
    </property>
   
    <bag name="l_meal_content" cascade="all-delete-orphan" inverse="true" generic="true">
      <key>
        <column name="meal_id" />
      </key>
      <one-to-many class="meal_content" />
    </bag>

    <bag name="l_meal_symptom" cascade="all-delete-orphan" inverse="true" generic="true">
      <key>
        <column name="meal_id" />
      </key>
      <one-to-many class="meal_symptom" />
    </bag>
  </class>
</hibernate-mapping>