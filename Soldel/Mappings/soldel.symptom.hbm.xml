<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Soldel" namespace="mupeModel" xmlns="urn:nhibernate-mapping-2.2">
  <class name="symptom" table="symptom">
    <id name="symptom_id" type="Int64">
      <column name="symptom_id" not-null="true" />
      <generator class="assigned" />
    </id>
    <property name="symptom_cat" type="Int64">
      <column name="symptom_cat" not-null="true" />
    </property>
    <property name="value" type="String">
      <column name="value" not-null="true" />
    </property>
    <property name="symptom_date" type="DateTime">
      <column name="symptom_date" not-null="true" />
    </property>

    <bag name="l_meal_symptom" cascade="all-delete-orphan" inverse="true" generic="true">
      <key>
        <column name="symptom_id" />
      </key>
      <one-to-many class="meal_symptom" />
    </bag>
  </class>
</hibernate-mapping>